%h2
  = t '.resend_unlock'

= form_for(resource, as: resource_name, url: unlock_path(resource_name), html: { method: :post }) do |f|
  = devise_error_messages!

  .field
    = f.label :email
    %br
    = f.email_field :email, autofocus: true

  .actions
    = f.submit t('.resend_unlock')

= render "devise/shared/links"